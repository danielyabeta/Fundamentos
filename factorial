float calculodelarea (float l);

void main()
{
	float area,l;
	cout<<"ingrese lado: "<<endl;
	cin>>l;
	area=calculodelarea(l);
	cout<<"area"<<area<<endl;
	getch();
}

float calculodelarea (float l)
{
	float area;
	  area=l*l;
	  return area;
}
